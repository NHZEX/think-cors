{
    "name": "zxin/think-cors",
    "type": "library",
    "description": "",
    "keywords": [
        "thinkphp6",
        "thinkphp",
        "cors"
    ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "auooru",
            "email": "auooru@outlook.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/polyfill-php80": "^1",
        "topthink/framework": "^6.1|~8.0.1|~8.1.2"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.22",
        "phpstan/phpstan": "^1.10.28",
        "phpunit/phpunit": "^9",
        "symfony/var-dumper": "^5.4|^6.0"
    },
    "autoload": {
        "psr-4": {
